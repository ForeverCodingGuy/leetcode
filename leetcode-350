class Solution {
public:
    vector<int> intersect(vector<int>& nums1, vector<int>& nums2) 
    {
        vector<int> rsu;
        unordered_map<int,int> tem;
        for(auto i:nums1)
        {
            tem[i]++;
        }
        for(auto j:nums2)
        {
            if(tem[j]>0)
            {
                rsu.push_back(j);
                tem[j]--;
            }
        }
        return rsu;
    }
};
