class Solution {
public:
    void rotate(vector<vector<int>>& matrix) 
    {
        int sz=matrix.size();
        for(auto i=0;i<sz-1;i++)
        {
            for(auto j=0;j<sz-1-i;j++)
            {
                swap(matrix[i][j],matrix[sz-1-j][sz-1-i]);
            }
        }
        for(auto k=0;k<sz;k++)
        {
            for(auto m=0;m<sz/2;m++)
            {
                swap(matrix[m][k],matrix[sz-1-m][k]);
            }
        }
    }
};
