class Solution {
public:
    int maxSubArray(vector<int>& nums) 
    {
        int rsu = INT_MIN;
        int count = 0;
        for(int i = 0;i < nums.size();i++)
        {
            count += nums[i];
            if(count > rsu) rsu = count;
            if(count < 0) count = 0;
        }
        return rsu;
    }
};
