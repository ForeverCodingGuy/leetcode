class Solution {
public:
    bool canConstruct(string ransomNote, string magazine) 
    {
        int record[26]={0};
        for(auto& i:magazine)
        {
            record[i-'a']++;
        }
        for(auto& j:ransomNote)
        {
            record[j-'a']--;
        }
        for(int k=0;k<26;k++)
        {
            if(record[k]<0)
            {
                return false;
            }
        }
        return true;
    }
};
