class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) 
    {
        unordered_map<int,int> s;
        for(int i=0;i<nums.size();i++)
        {
            auto itr=s.find(target-nums[i]);
            if(itr!=s.end())
            {
                return {i,itr->second};
            } 
            s.insert(pair<int,int>(nums[i],i));
        }
        return {};
    }
};
