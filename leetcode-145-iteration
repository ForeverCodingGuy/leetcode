class Solution {
public:
    vector<int> inorderTraversal(TreeNode* root) 
	{
        vector<int> rsu;
        TreeNode* node = root;
        if(!root)  return rsu;
        stack<TreeNode*> st;
        st.push(node);
        while(!stack.empty())
        {
        	node = stack.top();
        	rsu.push_back(node -> val);
        	st.pop();
        	if(node -> left) st.push(node -> left);
        	if(node -> right) st.push(node -> right);
		}
		reverse(rsu.begin(),rsu.end());
		return rsu;
    }
};
