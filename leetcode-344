class Solution {
public:
    void reverseString(vector<char>& s) 
    {
        char tem;
        int sz=s.size();
        for(int i=0;i<sz/2;i++)
        {
            tem=s[i];
            s[i]=s[sz-i-1];
            s[sz-i-1]=tem;
        }
        for(auto k:s)
        {
            cout<<k;
        }
    }
};
